{
  "name": "gestor-cobranca.app",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=127.0.0.1 --port 8080 --proxy-config=proxy.conf.json --open",
    "start:prod": "node server.js",
    "build": "ng build --configuration production",
    "build:dev": "ng build",
    "serve:local": "ng serve --host=127.0.0.1 --proxy-config=proxy.conf.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "ng build --configuration production"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.6",
    "@angular/build": "^20.3.6",
    "@angular/cdk": "^20.2.12",
    "@angular/cli": "^20.3.6",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/forms": "^20.3.6",
    "@angular/material": "^20.2.12",
    "@angular/platform-browser": "^20.3.6",
    "@angular/router": "^20.3.6",
    "@angular/service-worker": "^20.3.6",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "jest-editor-support": "*",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.26.3",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0"
  }
}
